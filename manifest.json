{
    "name": "Steam Offers",
    "description": "List all Steam Offers so you can see it anywhere, and also a search for your favorite game!",
    "version": "1.3.1",
    "manifest_version": 2,
    "icons": {
        "128": "./images/128.png"
    },
    "browser_action":{
      "default_icon": "./images/128.png",
      "default_popup": "index.html"
    },
    "permissions": [
        "https://node-crawler-steam-offers.herokuapp.com/"
    ]
}
